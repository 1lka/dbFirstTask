<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="140.0" prefWidth="360.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dbbest.kirilenko.view.ProgramSettingsView">
   <children>
      <TextField fx:id="projPath" layoutX="155.0" layoutY="14.0">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></TextField>
      <TextField fx:id="logPath" layoutX="155.0" layoutY="62.0">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></TextField>
      <Label layoutX="14.0" layoutY="14.0" text="Default project folder:">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></Label>
      <Label layoutX="14.0" layoutY="62.0" text="Default logs file:">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></Label>
      <Button layoutX="304.0" layoutY="14.0" mnemonicParsing="false" onAction="#chooseProjectFolder" text="Select">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></Button>
      <Button layoutX="304.0" layoutY="62.0" mnemonicParsing="false" onAction="#chooseLogsFile" text="Select">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></Button>
      <Button layoutX="253.0" layoutY="100.0" mnemonicParsing="false" onAction="#saveChanges" text="Save">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></Button>
      <Button layoutX="296.0" layoutY="100.0" mnemonicParsing="false" onAction="#cancelChanges" text="Discard">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></Button>
   </children>
</AnchorPane>
